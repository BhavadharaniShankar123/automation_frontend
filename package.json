{
  "name": "test-automation",
  "displayName": "Test Automation",
  "description": "A VS Code extension for automation",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:test-automation.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "test-automation.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.96.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
